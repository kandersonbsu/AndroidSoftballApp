<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SignupActivity">

    <Button
            android:text="@string/ok"
            android:layout_width="124dp"
            android:layout_height="wrap_content"
            android:id="@+id/OK" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginBottom="64dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.501"/>
    <TextView
            android:text="@string/team_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            app:layout_constraintBaseline_toBaselineOf="@+id/TeamNameText" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/TeamNameText"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Team Name"
            android:ems="10"
            android:id="@+id/TeamNameText" android:layout_marginTop="44dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:autofillHints=""
            app:layout_constraintStart_toEndOf="@+id/textView4" app:layout_constraintHorizontal_bias="0.5"/>
    <TextView
            android:text="@string/location"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintBaseline_toBaselineOf="@+id/editText7" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/editText7"/>
    <EditText
            android:layout_width="284dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Location"
            android:ems="10"
            android:id="@+id/editText7"
            android:autofillHints=""
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/TeamNameText"
    />
    <TextView
            android:text="Manager Name:"
            android:layout_width="0dp"
            android:layout_height="15dp"
            android:id="@+id/textView6"
            app:layout_constraintBaseline_toBaselineOf="@+id/editText8" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/editText8"/>
    <EditText
            android:layout_width="276dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editText8"
            tools:layout_editor_absoluteY="151dp" app:layout_constraintStart_toStartOf="@+id/editText7"/>
</android.support.constraint.ConstraintLayout>